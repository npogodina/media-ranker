<table class="table">
  <thead class="">
    <tr>
      <th scope="col">Votes</th>
      <th scope="col">Title</th>
      <th scope="col">Created By</th>
      <th scope="col">Published</th>
      <th scope="col">Upvote</th>
    </tr>
  </thead>
  <tbody>
    <% work_type.each do |work| %>
      
        <tr class="list-group-item-action">
          <th scope="row"> <%= work.votes.count %> </th>
          <td> <%= link_to work.title, work_path(work) %> </td>
          <td> <%= work.creator %> </td>
          <td> <%= work.publication_year %> </td>
          <td> <%= link_to "Upvote", upvote_path(work.id), method: :create, class: "btn btn-primary" %> </td>
        </tr>
      
    <% end %> 
  </tbody>
</table>